<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:c="urn:import:org.gwtbootstrap3.client.ui.gwt">

	<b:Panel styleName="dataContainer">
		<b:Panel styleName="tablePanel">
			<b:PanelHeader>
				<b:Heading size="H3" text="Film Table" />
			</b:PanelHeader>
			<b:PanelBody ui:field="panel">
				<c:DataGrid ui:field='dataGrid' />
			</b:PanelBody>
			<b:PanelFooter>
				<b:Heading size="H5"/>
				<b:Button ui:field='downloadButton' type="PRIMARY" icon="DOWNLOAD">Download</b:Button>
			</b:PanelFooter>
		</b:Panel>
	</b:Panel>
</ui:UiBinder> 